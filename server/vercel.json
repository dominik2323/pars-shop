{
  "env": {
    "API_KEY": "@pars-shop-firebase-api-key-production",
    "AUTH_DOMAIN": "@pars-shop-firebase-auth-domain-production",
    "DATABASE_URL": "@pars-shop-firebase-database-url-production",
    "PROJECT_ID": "@pars-shop-firebase-project-id-production",
    "STORAGE_BUCKET": "@pars-shop-firebase-storage-bucket-production",
    "MESSAGING_SENDER_ID": "@pars-shop-firebase-messaging-sender-id-production",
    "APP_ID": "@pars-shop-firebase-app-id-production"
  },
  "routes": [
    {
      "src": "/api/sender",
      "dest": "/api/sender.ts"
    },
    {
      "src": "/api/data",
      "dest": "api/data.ts"
    },
    {
      "src": "/.*",
      "status": 404 
    }
  ]
}
